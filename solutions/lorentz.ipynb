   "source": [
    "α = 10\n",
    "ρ = 28\n",
    "β = 8/3\n",
    "\n",
    "def ode(v, t, α, ρ, β):\n",
    "    x, y, z = v\n",
    "    return np.array([\n",
    "        # your code here\n",
    "        α * (y - x),\n",
    "        x * (ρ - z) - y,\n",
    "        x * y - β * z\n",
    "    ])\n",
    "\n",
    "t = np.linspace(0, 20, 2000)\n",
    "x, y, z = odeint(ode, (0.1, 0.1, 0.1), t, args=(α, ρ, β)).T"
   ]
  }
